{
  "name": "custom-color-theme",
  "displayName": "Custom Color Theme",
  "description": "Personal theme generated by themer from the My Color Set color set",
  "publisher": "Zabronax",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/LarsGKodehode/vs-code-custom-extensions.git"
  },
  "scripts": {
    "build": "mkdir -p build && rm -rf build/* && vsce package --out build/"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "custom-color-theme-dark",
        "uiTheme": "vs-dark",
        "path": "./themes/custom-color-theme-dark.json"
      },
      {
        "label": "custom-color-theme-light",
        "uiTheme": "vs",
        "path": "./themes/custom-color-theme-light.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/vsce": "^2.24.0"
  }
}